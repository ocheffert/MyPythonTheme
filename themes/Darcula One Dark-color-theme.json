{
	"name": "Darcula One Dark",
	"type": "dark",
	"colors": {
		"editor.background": "#242424",
		"editor.foreground": "#cccccc",
		"editor.selectionBackground": "#204182cc",
		"editor.lineHighlightBackground": "#ffffff0b",
		//"editor.activeLineNumber.foreground": "#cccccc",
		"editorGutter.background": "#ffffff0b",
		"titleBar.activeBackground": "#ffffff1a",
		"titleBar.inactiveBackground": "#ffffff10",
		"titleBar.activeForeground": "#cccccc",
		"sideBar.border": "#ffffff1a",
		"sideBar.background": "#242424",
		"statusBar.foreground": "#cccccc",
		"statusBar.background": "#ffffff1a",
		"tab.activeBackground": "#ffffff1a",
		"tab.inactiveBackground": "#ffffff0b",
		"tab.activeBorder": "#0c7d9d",
		"editorSuggestWidget.background": "#303030",
		"editorSuggestWidget.highlightForeground": "#cc6e2f",
		"editorSuggestWidget.selectedBackground": "#204182cc",
		"badge.background": "#ffffff1a",
		"activityBar.background": "#ffffff1a",
		"activityBarBadge.background": "#007acc"
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"module": "#e5c07b",
		"function": "#61AFEF",
		"parameter": "#E06C75",
		"variable": "#E06C75",
		// "variable": "#ABB2BF",
		"method": "#61AFEF",
		"class": "#b68c3e",
		"selfParameter": {
			"foreground": "#b68c3e",
			"fontStyle": "italic bold"
		},
		"property": {
			"foreground": "#9E7BB0",
			"fontStyle": "italic bold"
		},
		// "property": {"foreground": "#C678DD", "fontStyle": "italic bold"},
		"magicFunction": {
			"foreground": "#61AFEF",
			"fontStyle": "italic bold"
		},
		"class.builtin": "#e5c07b",
		"class.decoratorbuiltin": "#61AFEF"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#7F848E",
				"fontStyle": "italic"
			}
		},
		{
			"name": "for, if, ...",
			"scope": "keyword.control.flow.python, source.python",
			"settings": {
				"foreground": "#C678DD",
				"fontStyle": "italic"
			}
		},
		{
			"name": "import",
			"scope": "keyword.control.import.python, source.python",
			"settings": {
				"foreground": "#C678DD",
				"fontStyle": "italic"
			}
		},
		{
			"name": "strings",
			"scope": "string.quoted.docstring.multi.python, source.python",
			"settings": {
				"foreground": "#98C379",
				"fontStyle": "bold"
			}
		},
		{
			"name": "single quoted",
			"scope": "string.quoted.docstring.single.python, source.python",
			"settings": {
				"foreground": "#98C379",
				"fontStyle": "bold"
			}
		},
		{
			"name": "single quoted strings",
			"scope": "string.quoted.single.python",
			"settings": {
				"foreground": "#98C379",
				"fontStyle": "bold"
			}
		},
		{
			"name": ":",
			"scope": "punctuation.section.function.begin.python, meta.function.python, source.python",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": ")",
			"scope": "punctuation.definition.parameters.end.python, meta.function.parameters.python, meta.function.python, source.python",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			},
		},
		{
			"name": "parenthesis not in signature",
			"scope": "punctuation.parenthesis",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "ponctuation not in signature",
			"scope": "punctuation.definition.arguments",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": ".",
			"scope": "punctuation.separator.period.python, source.python",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "element separator",
			"scope": "punctuation.separator.element.python",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "arguments separator",
			"scope": "punctuation.separator.arguments.python",
			"settings": {
				"foreground": "#ABB2BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "=",
			"scope": "keyword.operator.assignment.python, source.python",
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "arithmetic operators",
			"scope": "keyword.operator.arithmetic.python, meta.function-call.arguments.python, meta.function-call.python, source.python",
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "def",
			"scope": "storage.type.function.python, meta.function.python, source.python",
			"settings": {
				"foreground": "#C678DD",
				"fontStyle": "bold"
			}
		},
		{
			"name": "class",
			"scope": "storage.type.class.python, meta.class.python, source.python",
			"settings": {
				"foreground": "#C678DD",
				"fontStyle": "bold"
			}
		},
		{
			"name": "numeric",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#D19A66",
				"fontStyle": "bold"
			}
		},
		{
			"name": "comparison operators",
			"scope": "keyword.operator.comparison.python, source.python",
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "variable in signatures",
			"scope": "variable.parameter.function-call.python",
			"settings": {
				"foreground": "#ff6600",
				"fontStyle": "bold"
			}
		},
		{
			"name": "logical operator",
			"scope": "keyword.operator.logical.python",
			"settings": {
				"foreground": "#C678DD",
				"fontStyle": "bold"
			}
		},
		{
			"name": "boolean",
			"scope": "constant.language.python, source.python",
			"settings": {
				"foreground": "#D19A66",
				"fontStyle": "bold"
			}
		},
		{
			"name": "decorators",
			"scope": "punctuation.definition.decorator.python",
			"settings": {
				"foreground": "#61AFEF",
				"fontStyle": "bold"
			}
		}
	]
}